{
  "name": "@cml/express-backend",
  "version": "0.6.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src/ --watch node_modules/.prisma --ext ts,js,json,node,prisma --ignore prisma/ ./src/index.ts",
    "start": "ts-node ./src/index.ts",
    "build": "npx tsc",
    "start-prod": "npm build && node ./dist/index.js",
    "prisma-generate": "npx prisma generate",
    "prisma-db-pull": "npx prisma db pull",
    "tsc": "npx tsc"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@quixo3/prisma-session-store": "^3.1.9",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.2",
    "helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.8.3",
    "nodemon": "^2.0.19",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
